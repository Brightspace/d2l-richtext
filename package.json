{
  "description": "Polymer-based web components for rendering D2L annotated richtext.",
  "keywords": [
    "d2l",
    "richtext"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/d2l-richtext.git"
  },
  "homepage": "https://github.com/Brightspace/d2l-richtext",
  "name": "d2l-richtext",
  "scripts": {
    "test": "npm run lint && cross-env LAUNCHPAD_BROWSERS=chrome polymer test --skip-plugin sauce",
    "lint": "npm run lint:wc && npm run lint:js",
    "lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "lint:wc": "polymer lint",
    "test:dev": "npm run lint && cross-env LAUNCHPAD_BROWSERS=chrome polymer test -p --skip-plugin sauce"
  },
  "bugs": {
    "url": "https://github.com/Brightspace/d2l-richtext/issues"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.1.3",
    "d2l-typography": "BrightspaceUI/typography#semver:^7",
    "eslint": "^4.17.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.2",
    "polymer-cli": "^1.9.2",
    "wct-browser-legacy": "^1.0.1"
  },
  "version": "2.0.2",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "main": "d2l-richtext.js",
  "dependencies": {
    "s-html": "Brightspace/s-html#semver:^2",
    "@polymer/polymer": "^3.0.0"
  }
}
